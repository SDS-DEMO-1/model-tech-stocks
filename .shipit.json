{
  "data" : {
    "module" : {
      "repositoryOwner" : "shipDataScience",
      "repositoryName" : "data-yahoo-finance", 
      "repositoryBranch" : "master"
    },
    "config" : {
      "tickers" : ["GOOG", "DBC", "QQQ"], 
      "lags" : [1,3,5],
      "startDate" : "2014-04-01"
    }
  },
  "scoring" : {
    "skipScoring" : false
  },
  "plugins" : [
    {
      "module" : {
        "repositoryOwner" : "shipDataScience",
        "repositoryName" : "Stability-Monitoring",
        "repositoryBranch" : "master"
      },
      "config": {
        "io" : {
          "strategy" : {
            "identifier" : "CSV",
            "options" : {
              "sep" : "\t"
            }
          }
        },
        "logging" : {
          "logLevel" : "INFO"
        },
        "report": {
          "period" : {
            "field" : "Date",
            "type" : "datetime",
            "benchmark" : {
              "startsAt" : "2014-04-01",
              "endsAt" : "2014-08-01"
            },
            "report" : {
              "startsAt" : "2014-08-01",
              "window" : {
                "type" : "calendar",
                "interval" : "monthly"
              },
              "discardLastPeriod" : true
            }
          },
          "fields" : {
            "included" : ["GOOG", "DBC", "QQQ"]
          },
          "quantiles": {
            "defaultNumberOfTiles" : 3
          }
        }
      }
    }
  ]
}




