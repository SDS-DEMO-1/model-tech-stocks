{
  "scheduler" : {
    "runEveryTimeIncrement" : "month",
    "runOnCodeChange" : true
  },
  "data" : {
    "cloneUrl" : "git@github.com:shipDataScience/data-yahoo-finance.git",
    "config" : {
      "tickers" : ["AAPL", "GOOG", "MSFT"], 
      "lags" : [1,3,5],
      "startDate" : "2014-04-01"
    }
  },
  "scoring" : {
    "skipScoring" : False
  },
  "plugins" : [
    {
      "alias" : "Stability",
      "cloneUrl" : "git@github.com:shipDataScience/Stability-Monitoring.git",
      "repositoryBranch" : "master",
      "config": {
        "io" : {
          "strategy" : {
            "identifier" : "CSV",
            "sep" : "\t"
          }
        },
        "report": {
          "period" : {
            "period_field" : "date",
            "period_type" : "time",
            "period_interval" : "30 day",
            "benchmark_starts_at" : "2012-04-01",
            "benchmark_ends_at" : "2012-07-31",
            "report_starts_at" : "2012-08-01"
          },
          "fields" : {
            "included" : ["GOOG", "MSFT", "AAPL"]
          },
          "quantiles": {
             "default" : 5
          }
        }
      }
    }
  ]
}




